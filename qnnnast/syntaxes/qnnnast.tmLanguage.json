{
	"scopeName": "source.qnnnast",
	"patterns": [
		{
			"name": "support.function.qnnnast",
			"match": "\\s*(program|import|declare_var|declare_attr|type|expression|operator|term|variable|name|value|call|use_typevar|typevar|declare_typevar|args_call|statements|func|args_declare|declare_arg|class|method|arr|tuple|dict|if|for|while|break|return)\\s?\\("
		},
		{
			"name": "variable.other.qnnnast",
			"match": "(?<=(\\(|,))\\s*\\w+\\s*(?=:)"
		},
		{
			"name": "string.quoted.double.qnnnast",
			"begin": "(?<=:)",
			"end": "(?=,)"
		},
		{
			"name": "comment.line.number-sign.qnnnast",
			"begin": "#",
			"end": "\n"
		},
		{
			"name": "comment.line.double-slash.qnnnast",
			"begin": "//",
			"end": "\n"
		},
		{
			"name": "comment.line.backtick.qnnnast",
			"begin": "/\\*",
			"end": "\\*/"
		}
	]
}